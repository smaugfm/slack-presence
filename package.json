{
  "name": "slack-presence",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9333",
  "scripts": {
    "express": "node -r ts-node/register server/index.ts --project tsconfig.json",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && webpack",
    "run-built": "node dist/main.js",
    "eject": "react-scripts eject",
    "eslint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "prettier": "prettier --write"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.14.0",
    "@emotion/babel-preset-css-prop": "^11.12.0",
    "@emotion/eslint-plugin": "^11.12.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.88",
    "@mui/material": "^5.8.6",
    "@mui/x-date-pickers": "^5.0.0-alpha.7",
    "@types/express-ws": "^3.0.4",
    "@types/lodash": "^4.14.190",
    "@types/morgan": "^1.9.5",
    "@types/node": "^18.7.14",
    "@types/node-schedule": "^2.1.4",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/validator": "^13.7.14",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@webpack-cli/generators": "^3.0.7",
    "axios": "^1.9.0",
    "body-parser": "^2.2.0",
    "bufferutil": "^4.0.9",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "customize-cra": "^1.0.0",
    "date-fns": "^2.28.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "express": "^5.1.0",
    "express-ws": "^5.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.4",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.1",
    "prettier": "^3.5.3",
    "pretty-ms": "^9.2.0",
    "puppeteer": "24.8.2",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-use": "^17.6.0",
    "react-use-websocket": "^4.13.0",
    "simple-node-logger": "p10tyr/simple-node-logger#b384f3a28eff0c8bb44712949a8eb5939bb24296",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typed-emitter": "^2.1.0",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.7.4",
    "use-debounce": "^10.0.4",
    "utf-8-validate": "^6.0.5",
    "utility-types": "^3.11.0",
    "validator": "^13.15.0",
    "valuelink": "^2.0.9",
    "web-vitals": "^5.0.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": "eslint --fix --max-warnings=0",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "rules": {
      "no-console": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "plugins": [
      "only-warn"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
